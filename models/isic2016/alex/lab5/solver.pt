net: "models/isic2016/alex/lab5/train.pt"
test_iter: 5
# make test net, but don't invoke it from the solver itself
test_interval: 999999999
#base_lr: 0.01
base_lr: 1e-3
lr_policy: "step"
gamma: 0.1
stepsize: 2000
display: 20
max_iter: 100000
momentum: 0.9
weight_decay: 0.0005
snapshot: 2000
snapshot_prefix: "output/isic2016/alex_lab5"
solver_mode: GPU
test_initialization: 0